    "@tanstack/react-table": "^8.15.3",
    "isomorphic-dompurify": "^2.7.0",
    "plotly.js": "^2.31.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-plotly.js": "^2.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
