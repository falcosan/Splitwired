{
  "name": "splitwired",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "init": "pip install --upgrade --no-cache-dir -r requirements.txt",
    "cache": "find . -type f -name '*.py[co]' -delete -o -type d -name __pycache__ -delete",
    "freeze": "pip freeze > requirements.txt",
    "updates": "python updater.py requirements.txt",
    "upgrade": "pip list --outdated --format=freeze | grep -v '^-e' | cut -d = -f 1 | xargs -n1 pip install -U && npm run freeze",
    "dev": "webpack --config webpack.config.js --watch",
    "build": "webpack --mode production",
    "server": "flask run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falcosan/Splitwired.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/falcosan/Splitwired/issues"
  },
  "homepage": "https://github.com/falcosan/Splitwired#readme",
  "dependencies": {
    "@tanstack/react-table": "^8.10.3",
    "isomorphic-dompurify": "^1.9.0",
    "plotly.js": "^2.26.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-plotly.js": "^2.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "babel-loader": "^9.1.3",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.8.1",
    "dotenv-webpack": "^8.0.1",
    "image-minimizer-webpack-plugin": "^3.8.3",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.4",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
